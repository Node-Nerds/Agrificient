<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Add Warehouse</title>

  <!-- Bootstrap core CSS -->
  <link rel="stylesheet" href="/bootstrap.min.css">

  <!-- Custom styles for this template -->
  <link rel="stylesheet" href="/warehouse/addwarehouse.css">
</head>

<body class="text-center">

  <form class="form-add" action="/wallet/normal_transfer" method="post">
    <img src="/logo.svg" alt="" width="150" height="150">
    <h3 class="text-primary mb-3">Transfer funds</h3>
    <div class="form-group text-left">
      <label for="reciever">Phone Number</label>
      <input type="tel" id="reciever" name="reciever" class="form-control" value="+91" required autofocus>
    </div>
    <div class="form-group text-left">
        <label for="amount">Amount</label>
        <input type="number" id="amount" name="amount" class="form-control" placeholder="Amount" required>
      </div>
      <div class="form-group text-left">
        <label for="desc">Description</label>
        <input type="text" id="desc" name="desc" class="form-control" placeholder="" required>
      </div>
    
    <button type="submit" class="btn btn-lg btn-primary"> Transfer </button>

  </form>

</body>

</html>



<script>
    let e = '<%-e%>';

    if(e!=""){
