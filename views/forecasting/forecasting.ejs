    <script>
        function submit() {

            var form = $("#forecastForm");
            var mainContainer = document.getElementById("myOutput");

            $.post("/forecasting", form.serialize(), function (output, status) {
                mainContainer.innerHTML = "<div class='row mt-3 mb-3'>" +
                  "<div class='col-12'>"+
                    "<div class='card'>"+
                      "<div class='card-body'>"+
                      "<h5 class='text-left text-muted font-weight-bold'>Tentative Production:</h5>"+
                      "<p class='card-text text-right font-weight-bold text-dark' style='font-size: 2rem;'>"+output[0].slice(1,output[0].length-1)+"</p>"+
                  "</div>"+
                "</div>"+
              "</div>" ;
            })
        }
    </script>

